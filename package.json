{
  "name": "ko-plus-frontend",
  "scripts": {
    "start": "webpack-dev-server --config build/webpack.config.dev.js",
    "start:qa": "PROXY_ENV=qa npm start",
    "start:uat": "PROXY_ENV=uat npm start",
    "start:prod": "PROXY_ENV=prod npm start",
    "build": "npx webpack --config build/webpack.config.prod.js",
    "analyzer": "cross-env bundle_analyzer_report=true npx webpack --config build/webpack.config.prod.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --coverage --maxWorkers=2",
    "postinstall": "yarn build:dll",
    "build:dll": "npx webpack --config build/webpack.dll.config.js",
    "audit-package": "yarn audit --level low;[[ $? -ge 2 ]] && exit 1 || exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
